    "@pika/plugin-build-node": "^0.6.1",
    "@pika/plugin-copy-assets": "^0.6.1",
    "@pika/plugin-simple-bin": "^0.6.1",
    "@pika/plugin-ts-standard-pkg": "^0.6.1",
    "@types/babel__traverse": "^7.0.7",
    "@types/mkdirp": "^0.5.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  },
  "publishConfig": {
